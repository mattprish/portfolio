          ]
        }
      ],
      "source": [
        "def qq(PassStatus, score):\n",
        "  aaa = []\n",
        "  b = 0\n",
        "  for i in range(1000):\n",
        "    if df.iloc[i][score] >= passmark:\n",
        "      aaa.append('P')\n",
        "    else:\n",
        "      aaa.append('F')\n",
        "  df[PassStatus] = aaa\n",
        "qq('Math_PassStatus', 'math score')\n",
        "qq('Reading_PassStatus', 'reading score')\n",
        "qq('Writing_PassStatus', 'writing score')\n",
        "\n",
        "#ура ура ура\n",
        "\n",
        "print('матешу сдали', df[df.Math_PassStatus == 'P'].gender.count(), 'и не сдали', 1000 - df[df.Math_PassStatus == 'P'].gender.count())\n",
        "print('ридинг сдали', df[df.Reading_PassStatus == 'P'].gender.count(), 'и не сдали', 1000 - df[df.Reading_PassStatus == 'P'].gender.count())\n",
        "print('райтинг сдали', df[df.Writing_PassStatus == 'P'].gender.count(), 'и не сдали', 1000 - df[df.Writing_PassStatus == 'P'].gender.count())\n"
      ]
