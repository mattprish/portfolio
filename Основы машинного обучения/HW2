        "dfi = []\n",
        "dfm = []\n",
        "for i in range(10):\n",
        "  dfi.append(pd.read_csv(\"Students_info_\"+str(i)+\".csv\"))\n",
        "for i in range(10):\n",
        "  dfm.append(pd.read_csv(\"Students_marks_\"+str(i)+\".csv\"))\n",
        "\n",
        "\n",
        "df = pd.DataFrame.copy(pd.DataFrame(dfi[0]))\n",
